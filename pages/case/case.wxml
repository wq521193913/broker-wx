<view class='case-view'>
  <block wx:for="{{caseShows}}">
    <view style='width:45%;display:inline-block;margin-left:10px;' bindtap='showExhibition'>
      <image src='{{item.caseImg}}' style='width:100%;height:100px;'></image>
      <label>{{item.caseTitle}}</label>
    </view>
    
  </block>

</view>
