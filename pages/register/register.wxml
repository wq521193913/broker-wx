<view class='register-chunk'>
  <form bindsubmit='registerSubmit'>
  
    <view class='register-item'>
      <label>*手机号码:</label>
      <input type='text' name='brokerPhone' value='13386536846'></input>
    </view>

    <view class='register-item'>
      <label>真实姓名:</label>
      <input type='text' name='brokerName'></input>
    </view>

    <view class='register-item'>
      <label>身份证号:</label>
      <input type='text' name='brokerIdCard'></input>
    </view>

    <view class='register-item'>
      <label>现住址:</label>
      <input type='text' name='brokerAddress'></input>
    </view>

    <view class='register-item'>
      <label>QQ号:</label>
      <input type='text' name='brokerQq'></input>
    </view>

    <view class='register-item'>
      <label>微博:</label>
      <input type='text' name='brokerWeibo'></input>
    </view>

    <view class='register-operation'>
      <button form-type='submit'>保存</button>
      <button bindtap='registerCancel'>取消</button>
    </view>
  
  </form>

</view>
